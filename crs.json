{
    "author": {
        "type": "string",
        "default": "0xMRTT",
        "description": "Author of the project",
        "placeholder": "0xMRTT",
        "question": "What is the author of the project?",
        "validators": [
            "^[a-zA-Z0-9]+([a-zA-Z0-9](_|-| )[a-zA-Z0-9])*[a-zA-Z0-9]*$"
        ],
        "error-message": "Author must start with 0x"
    },
    "username": {
        "type": "string",
        "default": "0xMRTT",
        "description": "Username of the author on github",
        "placeholder": "0xMRTT",
        "question": "What is the username of the author?",
        "validators": [
            "^[a-zA-Z0-9]+([a-zA-Z0-9](_|-)[a-zA-Z0-9])*[a-zA-Z0-9]*$"
        ]
        
    },
    "email": {
        "type": "string",
        "default": "mail@example.com",
        "description": "Email of the author",
        "placeholder": "mail@example.com",
        "question": "What is the email of the author?",
        "validators": [
            "^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$"
        ],
        "error-message": "Email must be valid"
    },
    "license": {
        "type": "select",
        "description": "License of the project",
        "placeholder": "MIT",
        "options": [
            "MIT",
            "Apache-2.0",
            "GNU GPLv3",
            "GNU AGPLv3",
            "GNU LGPLv3",
            "Mozilla Public License 2.0",
            "Boost Software License 1.0",
            "The Unlicense",
            "Eclipse Public License 1.0",
            "ISC",
            "Proprietary :(",
            "Other"
        ],
        "default": "MIT",
        "question": "What is the license of the project?"
    },
    "project_slug": {
        "type": "string",
        "default": "my-awesome-project",
        "description": "Slug of the project",
        "placeholder": "my-awesome-project",
        "question": "What is the slug of the project?",
        "validators": [
            "^[a-z_(]$"
        ]
    },
    "project_name": {
        "type": "string",
        "default": "My Awesome Project",
        "description": "Name of the project",
        "placeholder": "My Awesome Project",
        "question": "What is the name of the project?"
    },
    "linter": {
        "type": "multiselect",
        "description": "Linter to use",
        "placeholder": "Clippy",
        "options": [
            "Clippy",
            "Rustfmt"
        ],
        "default": "Clippy",
        "question": "What linter do you want to use?"
    },
    "version": {
        "type": "string",
        "default": "0.1.0",
        "description": "Version of the project",
        "placeholder": "0.1.0",
        "question": "What is the version of the project?"
    },
    "rust_version": {
        "type": "string",
        "default": "2021",
        "description": "Edition of Rust to use",
        "placeholder": "2021",
        "question": "What is the edition of Rust to use?"
    }
}